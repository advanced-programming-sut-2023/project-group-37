<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/Button.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.MainMenu">
    <children>
        <VBox alignment="CENTER" layoutX="1.0" layoutY="72.0" prefHeight="290.0" prefWidth="232.0" spacing="20.0">
            <children>
                <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#startGame" text="New game">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#enterProfileMenu" text="Profile Menu">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#logout" text="Logout">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#exit" text="Exit">
                    <font>
                        <Font size="13.0" />
                    </font>
                </Button>
            </children>
        </VBox>
        <Label alignment="CENTER" layoutX="43.0" layoutY="15.0" prefHeight="55.0" prefWidth="284.0" text="Stronghold Crusader">
            <font>
                <Font name="Bodoni MT" size="30.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
