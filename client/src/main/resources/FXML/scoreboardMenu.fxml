<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.menus.ScoreboardMenu">
    <children>
        <Label alignment="CENTER" layoutX="160.0" layoutY="-7.0" prefHeight="61.0" prefWidth="262.0" text="Scoreboard"
               textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Segoe Print" size="26.0"/>
            </font>
        </Label>
        <VBox fx:id="ranks" layoutX="110.0" layoutY="107.0" prefHeight="278.0" prefWidth="61.0"/>
        <Label alignment="CENTER" layoutX="98.0" layoutY="59.0" prefHeight="46.0" prefWidth="77.0" text="Rank"
               textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Segoe Print" size="18.0"/>
            </font>
        </Label>
        <VBox fx:id="usernames" layoutX="182.0" layoutY="107.0" prefHeight="278.0" prefWidth="118.0"/>
        <Label alignment="CENTER" layoutX="185.0" layoutY="59.0" prefHeight="46.0" prefWidth="102.0" text="Username"
               textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Segoe Print" size="18.0"/>
            </font>
        </Label>
        <VBox fx:id="highscores" layoutX="310.0" layoutY="107.0" prefHeight="278.0" prefWidth="78.0"/>
        <Label alignment="CENTER" layoutX="298.0" layoutY="59.0" prefHeight="46.0" prefWidth="101.0" text="Highscore"
               textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Segoe Print" size="18.0"/>
            </font>
        </Label>
        <Button layoutX="15.0" layoutY="354.0" mnemonicParsing="false" onMouseClicked="#enterMainMenu" prefHeight="30.0"
                prefWidth="83.0" stylesheets="@../CSS/registration.css" text="Back"/>
        <VBox fx:id="avatars" layoutX="412.0" layoutY="107.0" prefHeight="278.0" prefWidth="78.0"/>
        <Label alignment="CENTER" layoutX="400.0" layoutY="59.0" prefHeight="46.0" prefWidth="101.0" text="Avatars"
               textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Segoe Print" size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
