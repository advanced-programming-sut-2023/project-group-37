<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="601.0" stylesheets="@../CSS/GameStyle.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.MainMenu">
    <children>
        <VBox alignment="CENTER" layoutX="1.0" layoutY="72.0" prefHeight="408.0" prefWidth="232.0">
            <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#startGame" text="Offline game" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#createLobby" text="Creat a lobby" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#joinLobby" text="Join a lobby" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#enterProfileMenu" text="Profile Menu" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#logout" text="Logout" />
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#exit" text="Exit" />
            </children>
        </VBox>
        <Label alignment="CENTER" layoutX="43.0" layoutY="15.0" prefHeight="55.0" prefWidth="284.0" text="Stronghold Crusader">
            <font>
                <Font name="Bodoni MT" size="30.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
