<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/GameStyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.LobbyMenu">
    <children>
        <VBox fx:id="usernames" alignment="TOP_CENTER" layoutX="262.0" layoutY="30.0" prefHeight="182.0" prefWidth="137.0" spacing="20.0" stylesheets="@../CSS/GameStyle.css">
            <children>
                <Label alignment="CENTER" prefHeight="27.0" prefWidth="138.0" text="Username" textFill="BLUE">
                    <font>
                        <Font name="Arial Black" size="18.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <VBox fx:id="territories" alignment="TOP_CENTER" layoutX="160.0" layoutY="30.0" prefHeight="168.0" prefWidth="90.0" spacing="20.0">
            <children>
                <Label alignment="CENTER" prefHeight="26.0" prefWidth="94.0" text="Territory" textFill="BLUE">
                    <font>
                        <Font name="Arial Black" size="18.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <Label fx:id="lobbyName" alignment="CENTER" layoutX="10.0" layoutY="30.0" prefHeight="27.0" prefWidth="146.0" text="Lobby " textFill="BLUE">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Label>
        <Button layoutX="4.0" layoutY="307.0" minWidth="146.0" mnemonicParsing="false" onMouseClicked="#leaveLobby" prefHeight="71.0" prefWidth="146.0" text="Leave" textFill="RED">
            <font>
                <Font name="Arial Black" size="15.0" />
            </font>
        </Button>
        <Label fx:id="lobbyCapacity" alignment="CENTER" layoutX="-2.0" layoutY="70.0" prefHeight="27.0" prefWidth="146.0" text="Capacity " textFill="BLUE">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Label>
        <AnchorPane fx:id="chatPane" layoutX="393.0" layoutY="2.0" prefHeight="394.0" prefWidth="210.0">
            <children>
                <VBox fx:id="chatVBox" layoutX="-1.0" layoutY="23.0" prefHeight="336.0" prefWidth="213.0" />
                <TextField fx:id="chatBox" layoutX="2.0" layoutY="368.0" prefHeight="27.0" prefWidth="169.0" promptText="Message" />
                <Rectangle fx:id="sendButton" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="27.0" layoutX="176.0" layoutY="367.0" onMouseClicked="#send" stroke="BLACK" strokeType="INSIDE" width="30.0" />
            <Label alignment="CENTER" layoutX="29.0" layoutY="-1.0" prefHeight="27.0" prefWidth="146.0" text="Chat" textFill="BLUE">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
